{
  "name": "@bsgbryan/rnzlr",
  "version": "25.8.13",
  "description": "A fun way to build apps for the web",
  "repository": {
    "type": "git",
    "url": "git@github.com:bsgbryan/rnzlr.git"
  },
  "scripts": {
  	"build": "tsc -p tsconfig.json"
  },
  "keywords": [],
  "author": "Bryan Maynard <bsgbryan@gmail.com> (https://bsgbryan.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bsgbryan/rnzlr/issues"
  },
  "exports": {
    ".": {
        "require": {
            "types": "./lib/index.ts",
            "default": "./lib/index.ts" 
        },
        "import": {
            "types": "./lib/index.ts",
            "default": "./lib/index.ts" 
        }
    },
    "./jsx-runtime": {
        "require": {
            "types": "./lib/types.ts",
            "default": "./dist/types.js"
        },
        "import": {
            "types": "./lib/types.ts",
            "default": "./dist/types.js"
        }
    },
	  "./jsx-dev-runtime": {
        "require": {
            "types": "./lib/types.ts",
            "default": "./dist/types.js"
        },
        "import": {
            "types": "./lib/types.ts",
            "default": "./dist/types.js"
        }
    }
  },
  "main": "./dist/index.js",
  "types": "./lib/types.ts",
  "homepage": "https://github.com/bsgbryan/rnzlr#readme",
  "dependencies": {
    "typescript": "5.9.2"
  }
}
